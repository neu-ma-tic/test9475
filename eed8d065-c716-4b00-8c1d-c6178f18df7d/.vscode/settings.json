{
  "deno.enable": true,
  "deno.lint": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
    "source.fixAll": true
  },
  "deno.unstable": true,
  "[javascript]": {
    "editor.defaultFormatter": "ms-vsliveshare.vsliveshare"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "ms-vsliveshare.vsliveshare"
  },
  "[json]": {
    "editor.defaultFormatter": "ms-vsliveshare.vsliveshare"
  },
  "[typescript]": {
    "editor.defaultFormatter": "ms-vsliveshare.vsliveshare"
  },
  "cSpell.words": [
    "Discordeno"
  ],
  "cSpell.enabled": false
}
